<% if user_signed_in? && current_user.likes?(story) %>
  <%= link_to "Unlike", story_like_path(story), method: :delete, remote: true %>
<% else %>
  <%= link_to "Like", story_like_path(story), method: :post, remote: true %>
<% end %>

<%= story.likes.count %>

<% story.likes.each do |like| %>
  <%= image_tag like.user.avatar_url, width: 20 %>
<% end %>
