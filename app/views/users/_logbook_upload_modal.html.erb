<!-- Modal -->
<div class="modal fade" id="logbookUploadModal" tabindex="-1" role="dialog" aria-labelledby="logbookUpload" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="logbookUpload">What logbook do you use?</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class='row m-2 h-100'>
        <div class="col-1">
          <%= image_tag('foreFlight.png', height: '40', width: '40') %>
        </div>
        <div class="col-5 my-auto">
          <div class="lb-provider-name pl-2">ForeFlight</div>
        </div>
        <div class="col-6 my-auto">
          <button class='btn btn-primary float-right' id="foreflight-lb">ForeFlight</button>
        </div>
      </div>
      <%= render 'users/upload_forms/foreflight' %>
      <div class='row m-2 h-100'>
        <div class="col-1">
          <%= image_tag('logtenpro.png', height: '40', width: '40') %>
        </div>
        <div class="col-5 my-auto">
          <div class="lb-provider-name pl-2">LogTen Pro</div>
        </div>
        <div class="col-6 my-auto">
          <button class='btn btn-primary float-right' id="logtenpro-lb">LogTen Pro</button>
        </div>
      </div>
      <%= render 'users/upload_forms/logtenpro' %>
      <!-- <div class='row m-2 h-100'>
        <div class="col-1">
          <%#= image_tag('mccPILOTLOG.png', height: '40', width: '40') %>
        </div>
        <div class="col-5 my-auto">
          <div class="lb-provider-name pl-2">mccPILOTLOG</div>
        </div>
        <div class="col-6 my-auto">
          <button class='btn btn-primary float-right' id="mccpilotlog-lb">mccPILOTLOG</button>
        </div>
      </div>
      <%#= render 'users/upload_forms/mccpilotlog' %> -->
    </div>
  </div>
</div>
